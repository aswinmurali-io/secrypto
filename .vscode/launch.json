{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Secrypto Server",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "SECRET_KEY": "testkey",
                "FLASK_APP": "secrypto",
                //"FLASK_ENV": "development",
                "FLASK_DEBUG": "0",
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true
        },
        {
            "name": "Secrypto Client",
            "type": "dart",
            "request": "launch",
            "program": "secrypto_app/lib/main.dart",
            "showMemoryUsage": true
        },
    ]
}